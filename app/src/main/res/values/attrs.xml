<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FixedAspectRatioView">
        <attr name="aspectRatioWidth" format="integer" />
        <attr name="aspectRatioHeight" format="integer" />
    </declare-styleable>


    <declare-styleable name="ScrimInsetsView">
        <attr name="insetForeground" format="reference|color" />
        <attr name="insetApplyPadding" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ForegroundLinearLayout">
        <attr name="foreground" format="reference" />
        <attr name="foregroundInsidePadding" format="boolean" />
        <attr name="foregroundGravity" format="integer" />
    </declare-styleable>

</resources>
