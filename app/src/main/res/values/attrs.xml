<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FixedAspectRatioView">
        <attr name="aspectRatioWidth" format="integer" />
        <attr name="aspectRatioHeight" format="integer" />
    </declare-styleable>


    <declare-styleable name="ScrimInsetsView">
        <attr name="insetForeground" format="reference|color" />
        <attr name="insetApplyPadding" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LinearLayoutOverlay">
        <attr name="overlay" format="reference" />
        <attr name="overlayInsidePadding" format="boolean" />
        <attr name="overlayGravity" format="integer" />
    </declare-styleable>

</resources>
